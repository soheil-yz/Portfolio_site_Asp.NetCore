<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyProtfolio</title>
    <link rel="stylesheet" href="~/css/skel.css" />
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="~/css/style-xlarge.css" />


</head>
<body id="top">
<header id="header">
    <a href="#" class="image avatar"> 
        <img src="~/images/avatar.jpg" alt=""/>
    </a>
    <h1>
            Im Soheil Yazdi<br>
       <div>Im Programmer And I Learn Pyhton And Csharp</div>
            <strong>Asp.NetCore</strong><br>
   
       
    </h1>

    @* Link Generation Using Url.Action *@
    @*<a href="@Url.Action("Index", "Home")" style="margin-left: 10px;">صفحه اصلی</a>
        <a href="@Url.Action("Contact", "Home")">تماس با من</a>*@

    @* Link Generation Using Tag Helpers *@
        <a asp-controller="Home" asp-action="Index" style="margin-left: 50px" ;>Home </a><br>
        <a asp-controller="Home" asp-action="Contact">Contact</a>

</header>
<dive id="main">
    @RenderBody()
</dive>
    <footer id="footer">
        <ul class="icons">
            <li>
                <a href="#" class="icon fa-twitter">
                    <span class="label">Twitter</span>
                </a>
            </li>
            <li>
                <a href="#" class="icon fa-github">
                    <span class="label">Github</span>
                </a>
            </li>
            <li>
                <a href="#" class="icon fa-dribbble">
                    <span class="label">Dribbble</span>
                </a>
            </li>
            <li>
                <a href="#" class="icon fa-envelope-o">
                    <span class="label">Email</span>
                </a>
            </li>
        </ul>
    </footer>





    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/jquery.poptrox.min.js"></script>
    <script src="~/js/skel.min.js" ></script>
    <script src="~/js/init.js" ></script>

 @RenderSection("Scripts", required: false)

</body>
</html>
